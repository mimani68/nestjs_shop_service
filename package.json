{
	"name": "shop",
	"version": "1.0.0",
	"description": "Simple shopping service",
	"author": "Mahdi Imani <imani.mahdi@gmail.com>",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "pretty-quick --staged --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug=5007 --watch",
		"start:prod": "node dist/src/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint&format": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"test": "jest --config jest.json",
		"test:e2e": "jest --silent --runInBand --forceExit --config test/e2e/jest-e2e.json",
		"test:cov": "jest --coverage --config jest.json --silent --runInBand --verbose",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --silent --runInBand --verbose --config jest.json"
	},
	"dependencies": {
		"@golevelup/ts-jest": "^0.3.4",
		"@nestjs/axios": "^2.0.0",
		"@nestjs/common": "^9.3.9",
		"@nestjs/config": "2.3.1",
		"@nestjs/core": "9.3.9",
		"@nestjs/microservices": "9.3.9",
		"@nestjs/mongoose": "^10.0.2",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "^9.3.9",
		"@nestjs/schedule": "2.2.0",
		"@nestjs/swagger": "^6.2.1",
		"@nestjs/typeorm": "9.0.1",
		"@nestjsx/crud": "^5.0.0-alpha.3",
		"@nestjsx/crud-request": "^5.0.0-alpha.3",
		"@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
		"class-transformer": "^0.5.1",
		"class-validator": "0.14.0",
		"cryptr": "6.0.2",
		"mongoose": "^8.0.3",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "7.3.0",
		"swagger-ui-express": "^4.6.2",
		"typeorm": "0.3.12"
	},
	"devDependencies": {
		"@compodoc/compodoc": "^1.1.15",
		"@nestjs/cli": "^9.2.0",
		"@nestjs/schematics": "9.0.4",
		"@nestjs/testing": "9.3.9",
		"@types/bcryptjs": "2.4.2",
		"@types/cache-manager": "^4.0.2",
		"@types/cache-manager-ioredis": "^2.0.2",
		"@types/cron": "1.7.3",
		"@types/express": "4.17.13",
		"@types/jest": "27.0.1",
		"@types/node": "^16.9.1",
		"@types/supertest": "2.0.11",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "3.0.2",
		"@typescript-eslint/parser": "3.0.2",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "6.10.0",
		"eslint-plugin-import": "2.20.1",
		"eslint-plugin-prettier": "3.4.0",
		"jest": "29.3.1",
		"prettier": "2.8.0",
		"pretty-quick": "^3.1.3",
		"supertest": "6.3.3",
		"ts-jest": "29.0.3",
		"ts-loader": "9.4.2",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.1.2",
		"tslint": "6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "4.9.5"
	}
}
